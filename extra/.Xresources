!! GENERAL
urxvt*cursorBlink:      false
urxvt*scrollBar:        false
urxvt*depth:            15
urxvt*internalBorder:   50
urxvt*dynamicColors:    on
urxvt*perl-ext-common:  default,matcher,selection-to-clipboard
URxvt.geometry:         70x15
URxvt.url-launcher:     /usr/bin/xdg-open
URxvt.matcher.button:   1


!! FONTS
URxvt.font:             xft:Iosevka Term:pixelsize=16:style=regular:antialias=true
URxvt.boldFont:         xft:Iosevka Term:pixelsize=16:style=bold:antialias=true
URxvt.italicFont:       xft:Iosevka Term:pixelsize=16:style=italic:antialias=true
URxvt.boldItalicFont:   xft:Iosevka Term:pixelsize=16:style=bold italic:antialias=true

Xft.antialias:          true
Xft.hinting:            true
Xft.hintstyle:          hintslight
Xft.lcdfilter:          lcddefault
Xft.dpi:                96
Xft.rgba:               rgb


!! COLORS
*.foreground:           #F8F8F2
*.background:           #282A36
*cursorColor:           #F8F8F2

! black
*.color0:               #000000
*.color8:               #4D4D4D

! red
